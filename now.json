{
    "version": 2,   
    "alias": [
        "abigail.africa", "www.abigail.africa"
    ],
    "public": true,
    "builds": [
        {
            "src": "build.sh",
            "use": "@now/static-build",
            "config": {
                "distDir": "public"
            }
        }
    ],
    "routes": [
        {
            "src": "^(.*)$",
            "dest": "$1.html"
        }
    ]
}
